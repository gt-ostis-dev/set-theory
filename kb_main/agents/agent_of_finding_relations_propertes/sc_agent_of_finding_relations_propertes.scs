sc_agent_of_finding_relations_propertes
=> nrel_main_idtf:
	[sc-агент нахождения свойств отношения] 
	(* <- lang_ru;; *);
	[sc-agent of finding relations propertes] 
	(* <- lang_en;; *);

<- abstract_sc_agent;

=> nrel_primary_initiation_condition: 
	(sc_event_add_output_arc => question_initiated);

=> nrel_initiation_condition_and_result: 
	(..sc_agent_of_finding_relations_propertes_condition => ..sc_agent_of_finding_relations_propertes_result);

<= nrel_sc_agent_key_sc_elements: 
	{
	question_initiated;
	question;
	question_finding_relations_propertes
	};

=> nrel_inclusion: 
	.platform_independent_realization_of_sc_agent_of_finding_relations_propertes
	(*
	<- platform_independent_abstract_sc_agent;;
	<= nrel_sc_agent_program: 
		{
		agent_of_finding_relations_propertes;
		proc_of_finding_relations_propertes	
		};;
	-> sc_agent_of_finding_relations_propertes_scp   
		(* <- active_sc_agent;; *);;
	*);;

..sc_agent_of_finding_relations_propertes_condition
= [*
	question_finding_relations_propertes _-> .._question;;
	question_initiated _-> .._question;;
	question _-> .._question;;
	.._question _-> .._parameter;;
*];;

..sc_agent_of_finding_relations_propertes_result
= [*
	question_finding_relations_propertes _-> .._question;;
	question_finished _-> .._question;;
	question _-> .._question;;
	.._question _=> nrel_answer:: .._answer;;
	.._question _-> .._parameter;;
*];;
